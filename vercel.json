{
  "version": 2,
  "name": "edge-landings",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "barber-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "doctor-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "tattoo-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "coffee-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "fitness-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "auto-demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "success.html",
      "use": "@vercel/static"
    },
    {
      "src": "cancel.html",
      "use": "@vercel/static"
    },
    {
      "src": "login.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard.html",
      "use": "@vercel/static"
    },
    {
      "src": "signup.html",
      "use": "@vercel/static"
    },
    {
      "src": "forgot-password.html",
      "use": "@vercel/static"
    },
    {
      "src": "reset-password.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
